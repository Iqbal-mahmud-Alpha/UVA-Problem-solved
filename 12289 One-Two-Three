#include <bits/stdc++.h>
using namespace std;
#define faster                    \
    ios_base::sync_with_stdio(0); \
    cin.tie(0);                   \
    cout.tie(0);
#define ff first
#define ss second
#define pb push_back
#define gcd(a, b) __gcd(a, b)
#define lcm(a, b) (a * (b / gcd(a, b)))
#define all(a) a.begin(), a.end()
#define INF 1000000000
#define mod 1000000007
#define num 1414213570

/**----data type----*/
typedef long long int ll;
typedef unsigned long long int llu;

int main()
{
    int tc;
    cin>>tc;
    string s;
    while(tc--){
            cin>>s;
            int len=s.size();
            if(len==5){
                cout<<3<<endl;
            }
            else if(len==3){
                if(s[0]=='o' and s[1]=='n'){
                    cout<<1<<endl;
                }
                else if(s[0]=='o' and s[2]=='e'){
                    cout<<1<<endl;
                }
                else if(s[1]=='n' and s[2]=='e'){
                    cout<<1<<endl;
                }
                else cout<<2<<endl;
            }
    }
}

